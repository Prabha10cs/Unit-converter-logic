    public class notify_adapter extends BaseAdapter {

        private final Activity context;
        private final List<Items_convertor> items;

        public notify_adapter(Activity context, List<Items_convertor> items) {

            this.context = context;

            this.items = items;

        }

        @Override
        public int getCount() {
            return items.size();
        }

        @Override
        public Object getItem(int position) {
            return position;
        }

        @Override
        public long getItemId(int position) {
            return position;
        }

        @Override
        public View getView(final int position, View view, ViewGroup parent) {
            TextView value;
            TextView title;

            LayoutInflater inflater = context.getLayoutInflater();

            View rowView;

            rowView = inflater.inflate(R.layout.list_convertor, null, true);


            value = rowView.findViewById(R.id.value);
            title = rowView.findViewById(R.id.title);

            final Items_convertor item = items.get(position);
            value.setText(item.getvalue());
            title.setText(Html.fromHtml(item.getdesc()));

            return rowView;
        }


    }


    public class MyAdapter1 extends BaseAdapter {

        final Activity context;
        final String[] item;

        public MyAdapter1(Activity context, String[] items) {

            this.context = context;
            this.item = items;
        }

        @Override
        public int getCount() {
            return item.length;
        }

        @Override
        public Object getItem(int position) {
            return position;
        }

        @Override
        public long getItemId(int position) {
            return position;
        }

        @Override
        public View getView(int position, View convertView, ViewGroup parent) {

            LayoutInflater mInflater = (LayoutInflater) context.getSystemService(Context.LAYOUT_INFLATER_SERVICE);
            convertView = mInflater.inflate(android.R.layout.simple_spinner_dropdown_item, parent, false);

            TextView txt = convertView.findViewById(android.R.id.text1);

            txt.setText(Html.fromHtml(item[position]));


            return convertView;
        }
    }


    public void set_ada() {
        switch (str_value) {
            case "Length": {
                String[] str = {" mm (MilliMetre)", " cm (CentiMetre)", " in (Inch)", " ft (Foot)", " m (Metre)", " km (KiloMetre)", " mile (Mile)", " yd (Yard)"};
                movieList.clear();
                for (int i = 0; i < str.length; i++) {
