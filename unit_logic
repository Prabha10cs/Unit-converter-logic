package unitconverter;

import android.app.Activity;
import android.content.Context;
import android.os.Bundle;
import android.text.Editable;
import android.text.Html;
import android.text.InputType;
import android.text.TextWatcher;
import android.view.LayoutInflater;
import android.view.View;
import android.view.ViewGroup;
import android.view.inputmethod.InputMethodManager;
import android.widget.AdapterView;
import android.widget.BaseAdapter;
import android.widget.ListView;
import android.widget.TextView;

import androidx.appcompat.widget.AppCompatEditText;
import androidx.appcompat.widget.AppCompatSpinner;
import androidx.fragment.app.Fragment;

import java.util.ArrayList;
import java.util.List;

import nithra.tamilcalender.R;
import nithra.tamilcalender.Utils;


public class ConvertorFragment extends Fragment {

    public ConvertorFragment() {
        // Required empty public constructor
    }

    @Override
    public void onCreate(Bundle savedInstanceState) {
        super.onCreate(savedInstanceState);
    }

    AppCompatEditText edit_txt;

    ListView listt;

    final List<Items_convertor> movieList = new ArrayList<>();
    String str_value;
    AppCompatSpinner spin_year;

    final set_decimal_point set = new set_decimal_point();
    notify_adapter adapterr;

    @Override
    public View onCreateView(LayoutInflater inflater, ViewGroup container,
                             Bundle savedInstanceState) {
        // Inflate the layout for this fragment
        View rootView = inflater.inflate(R.layout.fragment_convertor, container, false);
        final Bundle data = this.getArguments();
        str_value = data.getString("title");
        spin_year = rootView.findViewById(R.id.spin_year);
        edit_txt = rootView.findViewById(R.id.edit_txt);
        listt = rootView.findViewById(R.id.listt);


        adapterr = new notify_adapter(getActivity(), movieList);
        listt.setAdapter(adapterr);



        if (str_value.equals("Binary")) {
            edit_txt.setInputType(InputType.TYPE_CLASS_NUMBER);
        }


        edit_txt.addTextChangedListener(new TextWatcher() {
            @Override
            public void beforeTextChanged(CharSequence s, int start, int count, int after) {

            }

            @Override
            public void onTextChanged(CharSequence s, int start, int before, int count) {

                if (edit_txt.getText().toString().length() > 0) {
                    switch (str_value) {
                        case "Length":
                            Lenght_calculate();
                            break;
                        case "Area":
                            Area_calculate();
                            break;
                        case "Weight":
                            Weight_calculate();
                            break;
                        case "Volume":
                            Volume_calculate();
                            break;
                        case "Angle":
                            Angle_calculate();
                            break;
                        case "Time":
                            Time_calculate();
                            break;
                        case "Speed":
                            Speed_calculate();
                            break;
                        case "Interest":
                            ///
                            break;
                        case "Temp":
                            Temp_calculate();
                            break;
                        case "Cooking":
                            Cooking_calculate();
                            break;
                        case "Fuel":
                            Fuel_calculate();
                            break;
                        case "Binary":
                            Binary_calculate();
                            break;
                        case "Blood":
                            Blood_calculate();
                            break;
                        case "Data":
                            Data_calculate();
                            break;
                        case "Work":
                            Work_calculate();
                            break;
                        case "Density":
                            Density_calculate();
                            break;
                        case "Pressure":
                            Pressure_calculate();
                            break;
                        case "Current":
                            Current_calculate();
                            break;
                        case "Power":
                            Power_calculate();
                            break;
                        case "Force":
                            Force_calculate();
                            break;
                    }

                }
            }

            @Override
            public void afterTextChanged(Editable s) {

            }
        });
        spin_year.setOnItemSelectedListener(new AdapterView.OnItemSelectedListener() {
            @Override
            public void onItemSelected(AdapterView<?> parent, View view, int position, long id) {
                ((InputMethodManager) getActivity().getSystemService(Context.INPUT_METHOD_SERVICE))
                        .hideSoftInputFromWindow(spin_year.getWindowToken(), 0);
                set_ada();
                if (edit_txt.getText().toString().length() > 0) {
                    switch (str_value) {
                        case "Length":
                            Lenght_calculate();
                            break;
                        case "Area":
                            Area_calculate();
                            break;
                        case "Weight":
                            Weight_calculate();
                            break;
                        case "Volume":
                            Volume_calculate();
                            break;
                        case "Angle":
                            Angle_calculate();
                            break;
                        case "Time":
                            Time_calculate();
                            break;
                        case "Speed":
                            Speed_calculate();
                            break;
                        case "Interest":
                            ///
                            break;
                        case "Temp":
                            Temp_calculate();
                            break;
                        case "Cooking":
                            Cooking_calculate();
                            break;
                        case "Fuel":
                            Fuel_calculate();
                            break;
                        case "Binary":
                            Binary_calculate();
                            break;
                        case "Blood":
                            Blood_calculate();
                            break;
                        case "Data":
                            Data_calculate();
                            break;
                        case "Work":
                            Work_calculate();
                            break;
                        case "Density":
                            Density_calculate();
                            break;
                        case "Pressure":
                            Pressure_calculate();
                            break;
                        case "Current":
                            Current_calculate();
                            break;
                        case "Power":
                            Power_calculate();
                            break;
                        case "Force":
                            Force_calculate();
                            break;
                    }

                }
            }

            @Override
            public void onNothingSelected(AdapterView<?> parent) {

            }
        });


        return rootView;
    }


    public void Lenght_calculate() {

        if (edit_txt.getText().toString().length() > 0) {
            if (edit_txt.getText().toString().length() == 1) {
                if (edit_txt.getText().toString().equals(".")) {
                    return;
                }
            }
            if (movieList.size() == 0){
                return;
            }
            float get_input = Float.parseFloat(edit_txt.getText().toString());

            switch (spin_year.getSelectedItemPosition()) {
                case 0: {
                    Items_convertor items_convertor = movieList.get(0);
                    items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                    items_convertor = movieList.get(1);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 10.000, 6, 0));

                    items_convertor = movieList.get(2);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.03937008, 6, 0));

                    items_convertor = movieList.get(3);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.003280840, 6, 0));

                    items_convertor = movieList.get(4);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 1000.000, 6, 0));

                    items_convertor = movieList.get(5);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 1000000.000, 6, 0));

                    items_convertor = movieList.get(6);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 6.2137119e-07, 6, 0));

                    items_convertor = movieList.get(7);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.001093613, 6, 0));

                    adapterr.notifyDataSetChanged();
                }
                break;
                case 1: {
                    //cm to other convertion:
                    Items_convertor items_convertor = movieList.get(0);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 0.10000, 6, 0));

                    items_convertor = movieList.get(1);
                    items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                    items_convertor = movieList.get(2);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.3937008, 6, 0));

                    items_convertor = movieList.get(3);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.03280840, 6, 0));

                    items_convertor = movieList.get(4);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 100.00, 6, 0));

                    items_convertor = movieList.get(5);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 100000.00, 6, 0));

                    items_convertor = movieList.get(6);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 6.2137119e-06, 6, 0));

                    items_convertor = movieList.get(7);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.010936, 6, 0));

                    adapterr.notifyDataSetChanged();
                }
                break;

                case 2: {
                    //inch to other convertion:
                    Items_convertor items_convertor = movieList.get(0);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 0.039370, 6, 0));

                    items_convertor = movieList.get(1);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 2.54, 6, 0));

                    items_convertor = movieList.get(2);
                    items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                    items_convertor = movieList.get(3);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.083333, 6, 0));

                    items_convertor = movieList.get(4);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 39.370, 6, 0));

                    items_convertor = movieList.get(5);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 39370, 6, 0));

                    items_convertor = movieList.get(6);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 1.57828281218333e-05, 6, 0));

                    items_convertor = movieList.get(7);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.027778, 6, 0));

                    adapterr.notifyDataSetChanged();
                }
                break;
                case 3: {

                    //feet to other convertion:
                    Items_convertor items_convertor = movieList.get(0);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 0.0032808, 6, 0));

                    items_convertor = movieList.get(1);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 0.032808, 6, 0));

                    items_convertor = movieList.get(2);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 12.000, 6, 0));

                    items_convertor = movieList.get(3);
                    items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                    items_convertor = movieList.get(4);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 3.2808, 6, 0));

                    items_convertor = movieList.get(5);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 3280.8, 6, 0));

                    items_convertor = movieList.get(6);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.00018939, 6, 0));

                    items_convertor = movieList.get(7);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.33333, 6, 0));

                    adapterr.notifyDataSetChanged();
                }
                break;
                case 4: {

                    //meter to other convertion:
                    Items_convertor items_convertor = movieList.get(0);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 0.0010000, 6, 0));

                    items_convertor = movieList.get(1);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 0.010000, 6, 0));

                    items_convertor = movieList.get(2);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 39.37008, 6, 0));

                    items_convertor = movieList.get(3);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 3.280840, 6, 0));

                    items_convertor = movieList.get(4);
                    items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                    items_convertor = movieList.get(5);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 1000.0, 6, 0));

                    items_convertor = movieList.get(6);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.0006213712, 6, 0));

                    items_convertor = movieList.get(7);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 1.0936, 6, 0));

                    adapterr.notifyDataSetChanged();
                }
                break;
                case 5: {
                    //kilometer to other convertion:
                    Items_convertor items_convertor = movieList.get(0);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 1e-06, 6, 0));

                    items_convertor = movieList.get(1);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 1e-05, 6, 0));

                    items_convertor = movieList.get(2);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 39370.08, 6, 0));

                    items_convertor = movieList.get(3);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 3280.8, 6, 0));

                    items_convertor = movieList.get(4);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 0.0010000, 6, 0));

                    items_convertor = movieList.get(5);
                    items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                    items_convertor = movieList.get(6);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.6213712, 6, 0));

                    items_convertor = movieList.get(7);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 1093.613, 6, 0));

                    adapterr.notifyDataSetChanged();
                }
                break;
                case 6: {

                    //mile to other convertion:
                    Items_convertor items_convertor = movieList.get(0);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 6.2137119e-07, 6, 0));

                    items_convertor = movieList.get(1);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 6.2137119e-06, 6, 0));

                    items_convertor = movieList.get(2);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 63360, 6, 0));

                    items_convertor = movieList.get(3);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 5280.000, 6, 0));

                    items_convertor = movieList.get(4);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 0.00062137, 6, 0));

                    items_convertor = movieList.get(5);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 0.62137, 6, 0));

                    items_convertor = movieList.get(6);
                    items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                    items_convertor = movieList.get(7);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 1760.000, 6, 0));

                    adapterr.notifyDataSetChanged();
                }
                break;
                case 7: {

                    //yard to other convertion:
                    Items_convertor items_convertor = movieList.get(0);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 0.0010936, 6, 0));

                    items_convertor = movieList.get(1);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 0.010936, 6, 0));

                    items_convertor = movieList.get(2);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 36.000, 6, 0));

                    items_convertor = movieList.get(3);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 3.0000, 6, 0));

                    items_convertor = movieList.get(4);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 1.0936, 6, 0));

                    items_convertor = movieList.get(5);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 1093.6, 6, 0));

                    items_convertor = movieList.get(6);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.00056818, 6, 0));

                    items_convertor = movieList.get(7);
                    items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                    adapterr.notifyDataSetChanged();
                }
                break;
            }
        }

    }

    @Override
    public void onResume() {
        super.onResume();
        next();
    }

    public void next() {
        switch (str_value) {
            case "Length": {
                String[] str = {" mm (MilliMetre)", " cm (CentiMetre)", " in (Inch)", " ft (Foot)", " m (Metre)", " km (KiloMetre)", " mile (Mile)", " yd (Yard)"};

                MyAdapter1 html_spinner = new MyAdapter1(getActivity(), str);
                spin_year.setAdapter(html_spinner);
                break;
            }
            case "Area": {
                String[] str = {" mm<sup><small>2</small></sup> (Square MilliMetre)",
                        " cm<sup><small>2</small></sup> (Square CentiMetre)",
                        " m<sup><small>2</small></sup> (Square Metre)",
                        " in<sup><small>2</small></sup> (Square Inch)",
                        " ft<sup><small>2</small></sup> (Square Foot)",
                        " yd<sup><small>2</small></sup> (Square Yard)",
                        " km<sup><small>2</small></sup> (Square KiloMetre)",
                        " ct (Cent)", " ac (Acre)", " hec (Hectare)"};

                MyAdapter1 html_spinner = new MyAdapter1(getActivity(), str);
                spin_year.setAdapter(html_spinner);
                break;
            }
            case "Weight": {
                String[] str = {" μg (MicroGram)", " mg (MilliGram)", " g (Gram)", " kg (KiloGram)", " lb (Pound)", " oz (Ounce)", " ton (Tonne)"};

                MyAdapter1 html_spinner = new MyAdapter1(getActivity(), str);
                spin_year.setAdapter(html_spinner);
                break;
            }
            case "Volume": {
                String[] str = {" ml (MilliLitre)", " dl (DeciLitre)", " l (Litre)", " gal-Im (gallon[Imperial])", " gal-dry[US] (gallon,dry[US])", " gal-liq[US] (gallon,Liquid[US])", " cm<sup><small>3</small></sup> (Cubic CentiMetre)", " m<sup><small>3</small></sup> (Cubic Metre)", " in<sup><small>3</small></sup> (Cubic Inch)", " ft<sup><small>3</small></sup> (Cubic Foot)", " yd<sup><small>3</small></sup> (Cubic Yard)"};

                MyAdapter1 html_spinner = new MyAdapter1(getActivity(), str);
                spin_year.setAdapter(html_spinner);
                break;
            }
            case "Angle": {
                String[] str = {"<sup><small>0</small></sup> (Degree)", " rad (radian)", " mil (mil)", " <sup><small>^</small></sup> g(grad)", " sign (sign)", " circle (Turn)", " R_agl (Right Angle)", " S (Sextant)"};

                MyAdapter1 html_spinner = new MyAdapter1(getActivity(), str);
                spin_year.setAdapter(html_spinner);
                break;
            }
            case "Time": {
                String[] str = {" ms (MilliSecond)", " sec (Second)", " min (Minute)", " hr (Hour)", " day (per Day)", " wk (Week)", " mon (Month)", " yr (Year)"};

                MyAdapter1 html_spinner = new MyAdapter1(getActivity(), str);
                spin_year.setAdapter(html_spinner);
                break;
            }
            case "Speed": {
                String[] str = {" m/s (Metre/Second)", " ft/s (Foot/Second)", " m/min (Metre/Minute)", " ft/min (Foot/Minute)", " km/min (KiloMetre/Minute)", " km/hr (KiloMetre/Hour)", " mile/hr (Mile/Hour)", " C[Vacuum] (Speed of light[Vacuum])", " mach (Speed of Sound[Air])"};

                MyAdapter1 html_spinner = new MyAdapter1(getActivity(), str);
                spin_year.setAdapter(html_spinner);
                break;
            }
            case "Interest":
                break;
            case "Temp": {
                String[] str = {"<sup><small>0</small></sup>C (Celsius)", "<sup><small>0</small></sup>F (Fahrenheit)", "k (kelvin)", "<sup><small>0</small></sup>R (Reaumur)"};

                MyAdapter1 html_spinner = new MyAdapter1(getActivity(), str);
                spin_year.setAdapter(html_spinner);
                break;
            }
            case "Cooking": {
                String[] str = {" C_Sp (Coffe Spoon)", " Tea_Sp (Tea_Spoon)", " Tab_Sp (Table Spoon)", " B[UK] (barrel[UK])", " bck[Uk] (bucket[UK])", " bck[US] (bucket[US])", " ml (MilliLitre)", " L (Litre)", " kL (KiloLitre)", " ML (MegaLitre)"};

                MyAdapter1 html_spinner = new MyAdapter1(getActivity(), str);
                spin_year.setAdapter(html_spinner);
                break;
            }
            case "Fuel": {
                String[] str = {" m/l (Metre/Litre)", " km/l (KiloMetre/Litre)", " mile/l[US] (mile/Litre[US])", " mile/gal[UK] (mile/gallon[UK])", " mile/gal[US] (mile/gallon[US])", " L/100km (Litre/100KiloMetre)"};

                MyAdapter1 html_spinner = new MyAdapter1(getActivity(), str);
                spin_year.setAdapter(html_spinner);
                break;
            }
            case "Binary": {


                String[] str = {" 0 & 1 (Binary)", " oct (Octal)", " dec (Decimal)", " Hex (HexaDecimal)"};

                MyAdapter1 html_spinner = new MyAdapter1(getActivity(), str);
                spin_year.setAdapter(html_spinner);
                break;
            }
            case "Blood": {
                String[] str = {" mg/dl (Milligrams/deciliter)", " mmol/l (millimoles/litre)", " Hb-A1c% (Hemoglobin A1c)", " mmol/mol (millimole/mole)"};

                MyAdapter1 html_spinner = new MyAdapter1(getActivity(), str);
                spin_year.setAdapter(html_spinner);
                break;
            }
            case "Data": {
                String[] str = {" b (bit)", " B (Byte)", " KB (KiloByte)", " MB (MegaByte)", " GB (GigaByte)", " TB (TeraByte)", " PB (PetaByte)", " EB (ExaByte)", " ZB (ZettaByte)", " YB (YottaByte)", " BB (BrontoByte)"};

                MyAdapter1 html_spinner = new MyAdapter1(getActivity(), str);
                spin_year.setAdapter(html_spinner);
                break;
            }
            case "Work": {
                String[] str = {" J (Joule)", " kJ (Kilo Joule)", " cal (Calorie)", " kcal[cal] (Kilo Calorie[cal])", " kW-h (KiloWatt-Hour)", " kgf-m (Kilogram-Force meter)", " in-lbf (Inch-Pounds Force)", " ft-lbf (Foot-Pounds Force)", " BTU (British Thermal Unit )"};

                MyAdapter1 html_spinner = new MyAdapter1(getActivity(), str);
                spin_year.setAdapter(html_spinner);
                break;
            }
            case "Density": {
                String[] str = {" mg/mm<sup><small>3</small></sup> (MilliGram/Cubic MilliMetre)", "mg/cm<sup><small>3</small></sup> (MilliGram/Cubic CentiMetre)", "mg/m<sup><small>3</small></sup> (MilliGram/Cubic Metre)", "g/mm<sup><small>3</small></sup> (Gram/Cubic MilliMetre)", "g/cm<sup><small>3</small></sup> (Gram/Cubic CentiMetre)", "g/m<sup><small>3</small></sup> (Gram/Cubic Metre)", "kg/mm<sup><small>3</small></sup> (KiloGram/Cubic MilliMetre)", "kg/cm<sup><small>3</small></sup> (KiloGram/Cubic CentiMetre)", "kg/m<sup><small>3</small></sup> (KiloGram/Cubic Metre)", "lb/gal[UK] (Pound/gallon[UK])", "lb/gal[US] (Pound/gallon[US])"};

                MyAdapter1 html_spinner = new MyAdapter1(getActivity(), str);
                spin_year.setAdapter(html_spinner);
                break;
            }
            case "Pressure": {
                String[] str = {" atm (Atmosphere)", " Pa (Pascal)", " hPa[mbar] (HectoPascal[mbar])", " kPa (KiloPascal)", " MPa (MegaPascal)", " b (Bar)"};

                MyAdapter1 html_spinner = new MyAdapter1(getActivity(), str);
                spin_year.setAdapter(html_spinner);
                break;
            }
            case "Current": {
                String[] str = {" pA (PicoAmpere)", " nA (NanoAmpere)", " μA (MicroAmpere)", " mA (MilliAmpere)", " A (Ampere)", " kA (KiloAmpere)", " MA (MegaAmpere)", " Emu[abA] (Electric multiple unit[abA])", " Esu[stA] (Electrostatic unit[stA])"};

                MyAdapter1 html_spinner = new MyAdapter1(getActivity(), str);
                spin_year.setAdapter(html_spinner);
                break;
            }
            case "Power": {
                String[] str = {" W (watt)", " kW (KiloWatt)", " MW (MegaWatt)", " GW (GigaWatt)", " hp-M (Horsepower[metric])", " hp-Ele (Horsepower[Electric])", " hp-W (Horsepower[Water])", " hp-IN (Horsepower[International])", " k-cal/hr (KiloCalorie/hour)", " cal/hr (Calorie/hour)", " J/hr (Joule/hour)", " BTU/hr (British Thermal Unit/hour)"};

                MyAdapter1 html_spinner = new MyAdapter1(getActivity(), str);
                spin_year.setAdapter(html_spinner);
                break;
            }
            case "Force": {
                String[] str = {" N (Newton)", " kN (KiloNewton)", " MN (MegaNewton)", " gf (gram-force)", " kgf (Kilogram-force)", " Tf-M (Ton-force[metric])", " J/cm (Joule/CentiMetre)", " J/m (Joule/Metre)", " lb-f (Pound force)"};


                MyAdapter1 html_spinner = new MyAdapter1(getActivity(), str);
                spin_year.setAdapter(html_spinner);
                break;
            }
        }
    }
    public void Area_calculate() {
