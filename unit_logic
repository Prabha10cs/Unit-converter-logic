    public void Time_calculate() {
        if (edit_txt.getText().toString().length() > 0) {
            if (edit_txt.getText().toString().length() == 1) {
                if (edit_txt.getText().toString().equals(".")) {
                    return;
                }
            }
            if (movieList.size() == 0){
                return;
            }
            float get_input = Float.parseFloat(edit_txt.getText().toString());

            switch (spin_year.getSelectedItemPosition()) {
                case 0: {
                    if (movieList.size() > 0) {
                        Items_convertor items_convertor = movieList.get(0);
                        items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                        items_convertor = movieList.get(1);
                        items_convertor.setvalue("" + set.set_decimal(get_input / 1000, 6, 0));

                        items_convertor = movieList.get(2);
                        items_convertor.setvalue("" + set.set_decimal(get_input / 60000, 6, 0));

                        items_convertor = movieList.get(3);
                        items_convertor.setvalue("" + set.set_decimal((get_input / 60000) / 60, 6, 0));

                        items_convertor = movieList.get(4);
                        items_convertor.setvalue("" + set.set_decimal((get_input / (1000 * 60 * 60 * 24)), 6, 0));

                        items_convertor = movieList.get(5);
                        items_convertor.setvalue("" + set.set_decimal((get_input / (1000 * 60 * 60 * 24 * 7)), 6, 0));

                        items_convertor = movieList.get(6);
                        items_convertor.setvalue("" + set.set_decimal((-get_input / (1000 * 60 * 60 * 24 * 30)), 6, 0));

                        items_convertor = movieList.get(7);
                        items_convertor.setvalue("" + set.set_decimal((get_input / (1000 * 60 * 60 * 24 * 30 * 12)), 6, 0));

                        adapterr.notifyDataSetChanged();
                    }
                }
                break;
                case 1: {
                    if (movieList.size() > 0) {
                        Items_convertor items_convertor = movieList.get(0);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 1000, 6, 0));

                        items_convertor = movieList.get(1);
                        items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                        items_convertor = movieList.get(2);
                        items_convertor.setvalue("" + set.set_decimal(get_input / 60, 6, 0));

                        items_convertor = movieList.get(3);
                        items_convertor.setvalue("" + set.set_decimal(get_input / 3600, 6, 0));

                        items_convertor = movieList.get(4);
                        items_convertor.setvalue("" + set.set_decimal(get_input / 86400, 6, 0));

                        items_convertor = movieList.get(5);
                        items_convertor.setvalue("" + set.set_decimal(get_input / 604800, 6, 0));

                        items_convertor = movieList.get(6);
                        items_convertor.setvalue("" + set.set_decimal(get_input / 2592000, 6, 0));

                        items_convertor = movieList.get(7);
                        items_convertor.setvalue("" + set.set_decimal(get_input / 31556952, 6, 0));

                        adapterr.notifyDataSetChanged();
                    }
                }
                break;

                case 2: {
                    if (movieList.size() > 0) {
                        Items_convertor items_convertor = movieList.get(0);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 60 * 1000, 6, 0));

                        items_convertor = movieList.get(1);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 60, 6, 0));

                        items_convertor = movieList.get(2);
                        items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                        items_convertor = movieList.get(3);
                        items_convertor.setvalue("" + set.set_decimal(get_input / 60, 6, 0));

                        items_convertor = movieList.get(4);
                        items_convertor.setvalue("" + set.set_decimal(get_input / 1440, 6, 0));

                        items_convertor = movieList.get(5);
                        items_convertor.setvalue("" + set.set_decimal(get_input / 10080, 6, 0));

                        items_convertor = movieList.get(6);
                        items_convertor.setvalue("" + set.set_decimal(get_input / 44640, 6, 0));

                        items_convertor = movieList.get(7);
                        items_convertor.setvalue("" + set.set_decimal(get_input / 525600, 6, 0));

                        adapterr.notifyDataSetChanged();
                    }
                }
                break;
                case 3: {
                    if (movieList.size() > 0) {
                        Items_convertor items_convertor = movieList.get(0);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 3600000, 6, 0));

                        items_convertor = movieList.get(1);
