    public void Pressure_calculate() {
        if (edit_txt.getText().toString().length() > 0) {
            if (edit_txt.getText().toString().length() == 1) {
                if (edit_txt.getText().toString().equals(".")) {
                    return;
                }
            }
            if (movieList.size() == 0){
                return;
            }
            float get_input = Float.parseFloat(edit_txt.getText().toString());

            switch (spin_year.getSelectedItemPosition()) {
                case 0: {

                    Items_convertor items_convertor = movieList.get(0);
                    items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                    items_convertor = movieList.get(1);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 101325.01, 6, 0));

                    items_convertor = movieList.get(2);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 1013.2501, 6, 0));

                    items_convertor = movieList.get(3);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 101.32501, 6, 0));

                    items_convertor = movieList.get(4);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.10132501, 6, 0));

                    items_convertor = movieList.get(5);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 1.0132501, 6, 0));

                    adapterr.notifyDataSetChanged();
                }
                break;
                case 1: {
                    Items_convertor items_convertor = movieList.get(0);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.0000098692316931, 6, 0));

                    items_convertor = movieList.get(1);
                    items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                    items_convertor = movieList.get(2);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.01, 6, 0));

                    items_convertor = movieList.get(3);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.001, 6, 0));

                    items_convertor = movieList.get(4);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.000001, 6, 0));

                    items_convertor = movieList.get(5);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.00001, 6, 0));

                    adapterr.notifyDataSetChanged();
                }
                break;

                case 2: {
                    Items_convertor items_convertor = movieList.get(0);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.00098692316931, 6, 0));

                    items_convertor = movieList.get(1);
