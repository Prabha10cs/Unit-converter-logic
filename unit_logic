                    items_convertor = movieList.get(5);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.001, 6, 0));

                    adapterr.notifyDataSetChanged();
                }
                break;
                case 3: {

                    Items_convertor items_convertor = movieList.get(0);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 0.0098692316931, 6, 0));

                    items_convertor = movieList.get(1);
