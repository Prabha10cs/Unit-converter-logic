    }

    public void Binary_calculate() {
        if (edit_txt.getText().toString().length() > 0) {
            if (edit_txt.getText().toString().length() == 1) {
                if (edit_txt.getText().toString().equals(".")) {
                    return;
                }
            }
            if (movieList.size() == 0){
                return;
            }
            String get_input = edit_txt.getText().toString();

            switch (spin_year.getSelectedItemPosition()) {
                case 0: {
                    if (get_input.matches("[01]+")) {
                        int bin = Integer.parseInt(get_input, 2);
                        Items_convertor items_convertor = movieList.get(0);
                        items_convertor.setvalue("" + get_input);

                        items_convertor = movieList.get(1);
                        items_convertor.setvalue("" + Integer.toOctalString(bin));

                        items_convertor = movieList.get(2);
                        items_convertor.setvalue("" + bin);

                        items_convertor = movieList.get(3);
                        items_convertor.setvalue("" + Integer.toHexString(bin));

                        adapterr.notifyDataSetChanged();
                    } else {
                        Utils.toast_center(getActivity(), "Pls enter the Binary values(0&1)");
                    }

                }
                break;
                case 1: {
                    if (get_input.matches("[01234567]+")) {
                        int bin = Integer.parseInt(get_input, 8);
                        Items_convertor items_convertor = movieList.get(0);
                        items_convertor.setvalue("" + Integer.toBinaryString(bin));

                        items_convertor = movieList.get(1);
                        items_convertor.setvalue("" + get_input);

                        items_convertor = movieList.get(2);
                        items_convertor.setvalue("" + bin);

                        items_convertor = movieList.get(3);
                        items_convertor.setvalue("" + Integer.toHexString(bin));

                        adapterr.notifyDataSetChanged();
                    } else {
                        Utils.toast_center(getActivity(), "Pls enter the Octal values(0-7)");
                    }

                }
                break;

                case 2: {
                    if (get_input.length() >= 9) {
                        //input_txt_view.setFilters(new InputFilter[] { new InputFilter.LengthFilter(9) });
                        Utils.toast_center(getActivity(), "Pls enter Minimum length is 9");
                    } else {
                        int bin = Integer.parseInt(get_input);
                        Items_convertor items_convertor = movieList.get(0);
                        items_convertor.setvalue("" + Integer.toBinaryString(bin));

                        items_convertor = movieList.get(1);
                        items_convertor.setvalue("" + Integer.toOctalString(bin));

                        items_convertor = movieList.get(2);
                        items_convertor.setvalue("" + get_input);

                        items_convertor = movieList.get(3);
                        items_convertor.setvalue("" + Integer.toHexString(bin));

                        adapterr.notifyDataSetChanged();
                    }
                }
                break;
                case 3: {
                    if (get_input.length() >= 8) {
                        Utils.toast_center(getActivity(), "Pls enter Minimum length is 7");

                    } else {
                        int bin = Integer.parseInt(get_input, 16);
                        Items_convertor items_convertor = movieList.get(0);
                        items_convertor.setvalue("" + Integer.toBinaryString(bin));

                        items_convertor = movieList.get(1);
                        items_convertor.setvalue("" + Integer.toOctalString(bin));

                        items_convertor = movieList.get(2);
                        items_convertor.setvalue("" + bin);

                        items_convertor = movieList.get(3);
                        items_convertor.setvalue("" + get_input);

                        adapterr.notifyDataSetChanged();
                    }
                }
                break;

            }
        }
    }

    public void Blood_calculate() {
        if (edit_txt.getText().toString().length() > 0) {
            if (edit_txt.getText().toString().length() == 1) {
                if (edit_txt.getText().toString().equals(".")) {
                    return;
                }
            }
            if (movieList.size() == 0){
                return;
            }
            float get_input = Float.parseFloat(edit_txt.getText().toString());

            switch (spin_year.getSelectedItemPosition()) {
                case 0: {

                    Items_convertor items_convertor = movieList.get(0);
                    items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                    items_convertor = movieList.get(1);
                    items_convertor.setvalue("" + set.set_decimal(get_input / 18.0182, 6, 0));
                    if (edit_txt.getText().toString().length() == 0) {
                        items_convertor = movieList.get(2);
                        items_convertor.setvalue("0");
                    } else {
                        items_convertor = movieList.get(2);
                        items_convertor.setvalue("" + set.set_decimal((get_input + 77.3) / 35.6, 6, 0));
                    }

                    items_convertor = movieList.get(3);
                    items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                    adapterr.notifyDataSetChanged();
                }
                break;
                case 1: {
                    Items_convertor items_convertor = movieList.get(0);
                    items_convertor.setvalue("" + set.set_decimal(get_input * 18.00, 6, 0));

                    items_convertor = movieList.get(1);
                    items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                    items_convertor = movieList.get(2);
                    items_convertor.setvalue("" + set.set_decimal((get_input + 4.29) / 1.98, 6, 0));

                    items_convertor = movieList.get(3);
                    items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                    adapterr.notifyDataSetChanged();
                }
                break;

                case 2: {
                    Items_convertor items_convertor = movieList.get(0);
                    items_convertor.setvalue("" + set.set_decimal((get_input * 35.6) - 77.3, 6, 0));

                    items_convertor = movieList.get(1);
                    items_convertor.setvalue("" + set.set_decimal((get_input * 1.98) - 4.29, 6, 0));

                    items_convertor = movieList.get(2);
                    items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                    items_convertor = movieList.get(3);
                    items_convertor.setvalue("" + set.set_decimal((get_input - 2.15) * 10.929, 6, 0));

                    adapterr.notifyDataSetChanged();
                }
                break;
                case 3: {

                    Items_convertor items_convertor = movieList.get(0);
                    items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                    items_convertor = movieList.get(1);
                    items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                    items_convertor = movieList.get(2);
                    items_convertor.setvalue("" + set.set_decimal((get_input + 4.29) / 1.98, 6, 0));

                    items_convertor = movieList.get(3);
                    items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                    adapterr.notifyDataSetChanged();
                }
                break;
            }
        }
    }

    public void Data_calculate() {
