                        items_convertor.setvalue("" + set.set_decimal(get_input * 54992.312075, 6, 0));

                        items_convertor = movieList.get(5);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 52834.410472, 6, 0));

                        items_convertor = movieList.get(6);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 1000000000.00, 6, 0));

                        items_convertor = movieList.get(7);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 1000000.00, 6, 0));

                        items_convertor = movieList.get(8);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 1000.00, 6, 0));

                        items_convertor = movieList.get(9);
                        items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                        adapterr.notifyDataSetChanged();
                    }
                }
                break;
            }
        }
    }

    public void Fuel_calculate() {
        if (edit_txt.getText().toString().length() > 0) {
            if (edit_txt.getText().toString().length() == 1) {
                if (edit_txt.getText().toString().equals(".")) {
                    return;
                }
            }
            if (movieList.size() == 0){
                return;
            }
            float get_input = Float.parseFloat(edit_txt.getText().toString());

            switch (spin_year.getSelectedItemPosition()) {
                case 0: {
                    if (movieList.size() > 0) {
                        Items_convertor items_convertor = movieList.get(0);
                        items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                        items_convertor = movieList.get(1);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 0.001, 6, 0));

                        items_convertor = movieList.get(2);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 0.0006213711922373, 6, 0));

                        items_convertor = movieList.get(3);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 0.002824809362797, 6, 0));

                        items_convertor = movieList.get(4);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 0.002352145832948, 6, 0));

                        items_convertor = movieList.get(5);
                        items_convertor.setvalue("" + set.set_decimal(100000 / get_input, 6, 0));

                        adapterr.notifyDataSetChanged();
                    }
                }
                break;
                case 1: {
                    if (movieList.size() > 0) {
                        Items_convertor items_convertor = movieList.get(0);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 1000, 6, 0));

                        items_convertor = movieList.get(1);
                        items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                        items_convertor = movieList.get(2);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 0.6213711922373, 6, 0));

                        items_convertor = movieList.get(3);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 2.824809362797, 6, 0));

                        items_convertor = movieList.get(4);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 2.352145832948, 6, 0));

                        items_convertor = movieList.get(5);
                        items_convertor.setvalue("" + set.set_decimal(100 / get_input, 6, 0));

                        adapterr.notifyDataSetChanged();
                    }
                }
                break;

                case 2: {
                    if (movieList.size() > 0) {
                        Items_convertor items_convertor = movieList.get(0);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 1609.344, 6, 0));

                        items_convertor = movieList.get(1);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 1.609344, 6, 0));

                        items_convertor = movieList.get(2);
                        items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                        items_convertor = movieList.get(3);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 4.546089999161, 6, 0));

                        items_convertor = movieList.get(4);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 3.785411783379, 6, 0));

                        items_convertor = movieList.get(5);
                        items_convertor.setvalue("" + set.set_decimal(62.1371 / get_input, 6, 0));

                        adapterr.notifyDataSetChanged();
                    }
                }
                break;
                case 3: {
                    if (movieList.size() > 0) {
                        Items_convertor items_convertor = movieList.get(0);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 354.00619, 6, 0));

                        items_convertor = movieList.get(1);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 0.35400619, 6, 0));

                        items_convertor = movieList.get(2);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 0.2199692483397, 6, 0));

                        items_convertor = movieList.get(3);
                        items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                        items_convertor = movieList.get(4);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 0.8326741846461, 6, 0));

                        items_convertor = movieList.get(5);
                        items_convertor.setvalue("" + set.set_decimal(282.480936 / get_input, 6, 0));

                        adapterr.notifyDataSetChanged();
                    }
                }
                break;
                case 4: {
                    if (movieList.size() > 0) {
                        Items_convertor items_convertor = movieList.get(0);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 425.1437075, 6, 0));

                        items_convertor = movieList.get(1);
