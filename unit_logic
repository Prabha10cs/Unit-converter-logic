    public void Temp_calculate() {
        if (edit_txt.getText().toString().length() > 0) {
            if (edit_txt.getText().toString().length() == 1) {
                if (edit_txt.getText().toString().equals(".")) {
                    return;
                }
            }
            if (movieList.size() == 0){
                return;
            }
            float get_input = Float.parseFloat(edit_txt.getText().toString());

            switch (spin_year.getSelectedItemPosition()) {
                case 0: {
                    if (movieList.size() > 0) {
                        Items_convertor items_convertor = movieList.get(0);
                        items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                        items_convertor = movieList.get(1);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 9 / 5 + 32, 6, 0));

                        items_convertor = movieList.get(2);
                        items_convertor.setvalue("" + set.set_decimal(get_input + 273.15, 6, 0));

                        items_convertor = movieList.get(3);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 0.80000, 6, 0));

                        adapterr.notifyDataSetChanged();
                    }
                }
                break;
                case 1: {
                    if (movieList.size() > 0) {
                        Items_convertor items_convertor = movieList.get(0);
                        items_convertor.setvalue("" + set.set_decimal((get_input - 32) * 5 / 9, 6, 0));

                        items_convertor = movieList.get(1);
                        items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                        items_convertor = movieList.get(2);
                        items_convertor.setvalue("" + set.set_decimal((get_input + 459.67) * 5 / 9, 6, 0));

                        items_convertor = movieList.get(3);
                        items_convertor.setvalue("" + set.set_decimal((get_input - 32) * 4 / 9, 6, 0));

                        adapterr.notifyDataSetChanged();
                    }
                }
                break;

                case 2: {
                    if (movieList.size() > 0) {
                        Items_convertor items_convertor = movieList.get(0);
                        items_convertor.setvalue("" + set.set_decimal(get_input - 273.15, 6, 0));

                        items_convertor = movieList.get(1);
                        items_convertor.setvalue("" + set.set_decimal((get_input - 273.15) * 1.8000 + 32.00, 6, 0));

                        items_convertor = movieList.get(2);
                        items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));

                        items_convertor = movieList.get(3);
                        items_convertor.setvalue("" + set.set_decimal((get_input - 273.15) * 0.80000, 6, 0));
                        adapterr.notifyDataSetChanged();
                    }
                }
                break;
                case 3: {
                    if (movieList.size() > 0) {
                        Items_convertor items_convertor = movieList.get(0);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 5 / 4, 6, 0));

                        items_convertor = movieList.get(1);
                        items_convertor.setvalue("" + set.set_decimal(get_input * 2.2500 + 32.00, 6, 0));

                        items_convertor = movieList.get(2);
                        items_convertor.setvalue("" + set.set_decimal(get_input / 0.80000 + 273.15, 6, 0));

                        items_convertor = movieList.get(3);
                        items_convertor.setvalue("" + set.set_decimal(get_input, 6, 0));
                        adapterr.notifyDataSetChanged();
                    }
                }
                break;
            }
        }
    }

